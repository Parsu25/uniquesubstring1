#include<stdio.h>
#include<stdlib.h>
int main()
{
    char a[1000];
    int l;
    scanf("%s",a);
    l=strlen(a);
    uniquesubstring(a,l);
}
int uniquesubstring(char a[],int l)
{
    int i,j,k=1;
    char b[1000]={""},c[1000]={""};
    for(i=0;i<l;i++)
    {
        c[0]=a[i];
        for(j=i+1;j<l;j++)
        {
            if(a[j]>a[i])
            {
               c[k++]=a[j]; 
            }
            else
            {
                break;
            }
        }
        if(strlen(c)>strlen(b))
        {
            strcpy(b,c);
            strcpy(c,"");
        }
        k=1;
    }
    printf("%s",b);
}
